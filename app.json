{
  "expo": {
    "name": "Meditate with Goenka",
    "slug": "meditatewithgoenka",
    "version": "1.0.0",
    "scheme": "Meditate with Goenka",
    "platforms": ["ios", "android"],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFAF7"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rglafarelle.goenka",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rglafarelle.goenka",
      "permissions": ["android.permission.MODIFY_AUDIO_SETTINGS"]
    },
    "owner": "rglafarelle",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d0b5fa25-438c-4d2e-bca5-bc83ffbf8cf5"
      }
    }
  }
}
